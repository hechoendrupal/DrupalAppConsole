{{ module }}.{{ entity }}.view:
  path: '/{{ module }}/{{ '{'~entity~'}' }}'
  defaults:
    _entity_view: '{{ entity }}'
    _title: '{{ entity }} Content'
  requirements:
    _entity_access: '{{ entity }}.view'
{{ module }}.{{ entity }}.list:
  path: '/admin/config/system/{{ module }}/{{ entity }}'
  defaults:
    _entity_list: '{{ entity }}'
    _title: '{{ entity }} List'
  requirements:
    _permission: 'view {{ entity }} entity'
{{ module }}.{{ entity }}.add:
  path: '/admin/config/system/{{ module }}/{{ entity }}/add'
  defaults:
    _entity_form: {{ entity }}.add
    _title: 'Add {{ entity }}'
  requirements:
    _entity_create_access: '{{ entity }}'
{{ module }}.{{ entity }}.edit:
  path: '/admin/config/system/{{ module }}/{{ entity }}/{{ '{'~entity~'}' }}/edit'
  defaults:
    _entity_form: {{ entity }}.edit
    _title: 'Edit {{ entity }}'
  requirements:
    _entity_access: '{{ entity }}.edit'
{{ module }}.{{ entity }}.delete:
  path: '/admin/config/system/{{ module }}/{{ entity }}/{{ '{'~entity~'}' }}/delete'
  defaults:
    _entity_form: {{ entity }}.delete
    _title: 'Delete {{ entity }}'
  requirements:
    _entity_access: '{{ entity }}.delete'
{{ module }}.{{ entity }}.settings:
  path: 'admin/structure/{{ module }}/manage/{{ entity }}'
  defaults:
   _form: '\Drupal\{{ module }}\Form\{{ entity }}SettingsForm'
   _title: '{{ entity }} Settings'
  requirements:
    _permission: 'administer {{ entity }} entity'