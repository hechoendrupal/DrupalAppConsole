{{ module }}.{{ entity }}.view:
  path: '/{{ module }}/{{ entity }}'
  defaults:
    _entity_view: '{{ module }}.{{ entity }}'
    _title: '{{ entity }} Content'
  requirements:
    _entity_access: '{{ module }}.{{ entity }}.view'
{{ module }}.{{ entity }}.list:
  path: '/admin/config/system/{{ module }}/{{ entity }}'
  defaults:
    _entity_list: '{{ module }}.{{ entity }}'
    _title: '{{ entity }} List'
  requirements:
    _permission: 'administer site configuration'
{{ module }}.{{ entity }}.add:
  path: '/admin/config/system/{{ module }}/{{ entity }}/add'
  defaults:
    _entity_form: {{ module }}.{{ entity }}.add
    _title: 'Add {{ entity }}'
  requirements:
    _entity_create_access: '{{ module }}.{{ entity }}'
{{ module }}.{{ entity }}.edit:
  path: '/admin/config/system/{{ module }}/{{ entity }}/{{ '{'~entity~'}' }}'
  defaults:
    _entity_form: {{ module }}.{{ entity }}.edit
    _title: 'Edit {{ entity }}'
  requirements:
    _entity_access: '{{ module }}.{{ entity }}.edit'  
{{ module }}.{{ entity }}.delete:
  path: '/admin/config/system/{{ module }}/{{ entity }}/{{ '{'~entity~'}' }}/delete'
  defaults:
    _entity_form: {{ module }}.{{ entity }}.delete
    _title: 'Delete {{ entity }}'
  requirements:
    _entity_access: '{{ module }}.{{ entity }}.delete'
{{ module }}.{{ entity }}.settings:
  path: 'admin/structure/{{ module }}/manage/{{ entity }}'
  defaults:
   _form: '\Drupal\{{ entity }}\Form\{{ entity }}SettingsForm'
   _title: '{{ entity }} Settings'
  requirements:
    _permission: 'administer {{ entity }} entity'