{{ entity }}.list:
  path: '/admin/config/system/{{ entity }}'
  defaults:
    _entity_list: '{{ entity }}'
    _title: '{{ entity }} Configuration'
  requirements:
    _permission: 'administer site configuration'
{{ entity }}.add:
  path: '/admin/config/system/{{ entity }}/add'
  defaults:
    _entity_form: '{{ entity }}.add'
    _title: 'Add {{ entity }}'
  requirements:
    _permission: 'administer site configuration'
{{ entity }}.edit:
  path: '/admin/config/system/{{ entity }}/{{ '{'~entity~'}' }}'
  defaults:
    _entity_form: '{{ entity }}.edit'
    _title: 'Edit {{ entity }}'
  requirements:
    _permission: 'administer site configuration'
{{ entity }}.delete:
  path: '/admin/config/system/{{ entity }}/{{ '{'~entity~'}' }}/delete'
  defaults:
    _entity_form: '{{ entity }}.delete'
    _title: 'Delete {{ entity }}'
  requirements:
    _permission: 'administer site configuration'

