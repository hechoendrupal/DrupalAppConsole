{% if service_name is defined %}
{% if not file_exists %}
services:
{% endif %}
  {{ service_name | lower }}:
    class: Drupal\{{module}}\{{ class_name }}
    arguments: [{{ servicesAsParametersKeys(services)|join(', ') }}]
    {% if tags %}
tags:
    {% for tag in tags %}
  - { {% for key, value in tag %} {{ key }}: {{ value }} , {% endfor %} }
    {% endfor %}
    {% endif %}
{% endif %}

