<?php

/**
 * @file
 * Contains {{module}}.module
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */

function {{module}}_help($path, $arg)
{
  switch ($path) {
    case 'admin/help#{{module}}':
      $output = '{{ description }}';
      return $output;
    break;
  }
}


/**
 * Implements hook_theme().
 */

function {{module}}_theme()
{
  $theme = [];

  return $theme;
}
